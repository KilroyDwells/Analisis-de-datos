x1 = [2,5,6,14]
y1= [11,22,33,44]

x2 = [2,5,6,15]
y2 = [4,12,18,45]

plt.plot(x1,y1, color = "blue", linewidth = 3, label = "Linea 1")
plt.plot(x1,y1, color = "green", linewidth = 3, label = "Linea 2")

plt.title("Dos Graficas juntas")
plt.xlabel("Eje X")
plt.ylabel("Eje Y")
plt.legend()
plt.grid()
plt.show()

